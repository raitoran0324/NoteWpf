<UserControl x:Class="_05101.BackGray"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_05101"
             mc:Ignorable="d" 
             d:DesignHeight="25" d:DesignWidth="25">
    <Grid>
        <Ellipse Width="25" Height="25" Fill="DimGray" Canvas.Left="10" Canvas.Top="25" Stroke="Gray" StrokeThickness="3" x:Name="Gray" MouseDown="Gray_MouseDown" VerticalAlignment="Top"></Ellipse>
    </Grid>
</UserControl>
